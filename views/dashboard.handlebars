<div class="container text-center">
  <h3>Latest Blog Posts</h3>
  <button id="edit-btn" class="btn btn-warning"><a href="/edit-blogs">Create a blog</a></button>
  ​
</div>
<div style="padding-left: 100px; padding-right:100px">
    {{#each blogs as |blog|}}
  <div class="card col-md-4" >
    <article>
    <div class="card-body">
        <a class="card-title" href="/view-blogs/{{id}}">{{title}}</a><br><br>
        <div>{{format_blog2 blog_post}}</div>
        <div class="card-subtitle mb-2 text-muted"><br>
            by {{user.username}} on {{format_date created_at}}
            |
            <br><a href="/view-blogs/{{id}}">{{comments.length}} {{format_plural "comment" comments.length}}</a>
        </div>
    </div>
</article>
  </div>
  {{/each}}
</div>
</div>
</div>
